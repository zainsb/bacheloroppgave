@model List<Studentparlamentet_28.Models.PersonvalgStemmer>

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />


</head>
<body>
    <div class="col-sm-12" id="box2">
        @Html.ActionLink(" ", "IndexEng", new { /* id=item.PrimaryKey */ }, new { @class = "btn btn-default glyphicon glyphicon-home btn-md" })
        <div class="form-inline" style="float:right;">
            @Html.ActionLink("Back", "HentResultatEng", new { /* id=item.PrimaryKey */ }, new { @class = "btn btn-default" })

        </div>
        <h2><b>An overview of all voteslips</b></h2>
        @if (Model.Count > 0)
        {
            <h3>
                Person Election with Electionnumber @Html.DisplayFor(model => Model[0].valgtypeid)
            </h3>
            <br /><br />
        }
        else
        {
            <h3>Person Election</h3>
            <br /><br />

        }
        <div id="tableHentResultat">
            <table class="table  table-responsive table-scrollable"  id="tableID">
                <thead class="thead-default">
                    <tr>
                        <th>
                            Voteslip
                        </th>
                        <th>
                            Firstname
                        </th>
                        <th>
                            Lastname
                        </th>
                    </tr>
                </thead>

                <tbody class="table-bordered">
                    @{int i = 1;}
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @i
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.fornavn)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.etternavn)
                            </td>

                        </tr>
                        i++;
                    }
                </tbody>
            </table>
            </div>
        </div>
        
</body>
</html>
