@model Studentparlamentet_28.Models.Valgtyper

<div class="panel panel-default">
    <div class="panel-body" id="resultatVotering">
        <table class="table">
            <tr>
                <th>ValgNr</th>
                <th>Valgtype</th>
                <th>Kjører </th>
            </tr>
            <tr>
                <td>@Model.valgtypeid</td>
                <td>@Model.valgtype</td>
                <td>
                    @if (@Model.start != false)
                    {
                        <p>Ja</p>
                    }
                    else
                    {
                        <p>Nei</p>
                    }
                </td>
            </tr>
        </table>

        <div class="panel-footer clearfix">
            <div class="pull-left">
                <a href="@Url.Action("ResultatVoteringID", "Bruker", new {  id = Model.valgtypeid })" class="btn btn-success ">Vis Resultat</a>
                <a href="#sletteModal" class="btn btn-large btn-danger" id="sletteKnapp"
                   data-navn="ValgNr: @Model.valgtypeid ValgType: @Model.valgtype"
                   data-id="@Model.valgtypeid" data-toggle="modal" data-sletteModal="true">Slett</a>
            </div>
        </div>
    </div>
</div>