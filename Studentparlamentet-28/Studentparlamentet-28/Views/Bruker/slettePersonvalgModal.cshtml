<div id="slettePersonvalgModal" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Bekreft sletting</h4>
            </div>


            <div class="modal-body">
                <p>Oversikt over valgdeltagere</p>

                <div class="alert alert-success">
                    @using (Html.BeginForm())
                    {
                        <p>Trykk slett for å fjerne Personvalg</p><div id="personvalgID"></div>
                        <p>Kandidater og resultat til dette personvalget vil bli slettet</p>
                        <input name="submit" type="submit" value="Slett" class="btn btn-success" />
                        <input type="hidden" id="valgtypeID3" name="valgtypeID2" />
                        <input type="hidden" id="stringID" name="stringID" />
                        
                    }
                </div>
            </div>



            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Lukk</button>
                

            </div>

        </div>
    </div>
</div>
