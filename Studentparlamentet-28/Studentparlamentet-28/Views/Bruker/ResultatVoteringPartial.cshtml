@model Studentparlamentet_28.Models.Valgtyper

<div class="panel panel-default">
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>ValgNr</th>
                <th>Valgtype</th>
                <th>Kjører </th>
            </tr>
            <tr>
                <td>@Model.valgtypeid</td>
                <td>@Model.valgtype</td>
                <td>@Model.start</td>
            </tr>
        </table>
    </div>
    <div class="panel-footer clearfix">
        <div class="pull-left">
            <a href="@Url.Action("Votering_Resultat", "Kunde", new { /* id=item.PrimaryKey */ })" class="btn btn-success ">Vis Resultat</a>
            <a href="#sletteModal" class="btn btn-large btn-danger" id="sletteKnapp"
               data-navn="ValgNr: @Model.valgtypeid ValgType: @Model.valgtype"
               data-id="@Model.valgtypeid" data-toggle="modal" data-sletteModal="true">Slett</a>
        </div>
    </div>
</div>