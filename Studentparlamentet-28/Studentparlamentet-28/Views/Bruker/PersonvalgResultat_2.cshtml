@model List<Studentparlamentet_28.Models.PersonvalgKandidatResultat>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
  
    <div class="col-sm-12" align="center" id="PersonvalgResultat">
        <div class="form-inline" style="float:left;">
            @Html.ActionLink(" ", "Index", new { /* id=item.PrimaryKey */ }, new { @class = "btn btn-default glyphicon glyphicon-home btn-md" })
            </div>
            <div class="form-inline" style="float:right;">
                @Html.ActionLink("Tilbake", "HentResultat", new { /* id=item.PrimaryKey */ }, new { @class = "btn btn-default" })

            </div>
            <br />
            <h1>Resultat av personvalg i kronologisk rekkefølge </h1>
            <br /><br />
            <div id="PersonvalgResultatContainer">
                @foreach (var item in Model)
            {

                    <ul class="list-group" id="ListeResultat" align="center">

                        <li style="list-style-type: none;"><h3>Kandidat</h3></li>
                        <li style="list-style-type: none;"><h1><b>@Html.DisplayFor(modelItem => item.fornavn) @Html.DisplayFor(modelItem => item.etternavn)</b></h1></li>
                        <li style="list-style-type: none;"><h3>Antall Stemmer</h3></li>
                        <li style="list-style-type: none;"><h1><b>@Html.DisplayFor(modelItem => item.stemmer)</b></h1></li>
                        
                    </ul>


                }
            </div>
        </div>

   
</body>
</html>











